#!/bin/sh
XULRUNNER_PATH=`find /bin | grep xulrunner*`

if [ -z "$XULRUNNER_PATH" ]; then
  XULRUNNER_PATH=`find /usr/bin | grep xulrunner*`
fi

if [ -z "$XULRUNNER_PATH" ]; then
  XULRUNNER_PATH=`find /usr/local/bin | grep xulrunner*`
fi

exec $XULRUNNER_PATH ${PROJECT_SOURCE_DIR}/gui/application.ini -printha-bin ${PROJECT_BINARY_DIR}/printha
